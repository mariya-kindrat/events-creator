# Test Environment Configuration (Example)
# Copy to .env.test or .env.local and fill in values. Do NOT commit real secrets.

# Test Database
DATABASE_TEST_URL="postgresql://USER:PASSWORD@HOST:PORT/DB_NAME?sslmode=require&channel_binding=require"
DATABASE_URL="${DATABASE_TEST_URL}"

# Test Environment Settings
NEXTAUTH_URL="https://your-test-branch.amplifyapp.com"
NEXTAUTH_SECRET="replace-with-strong-random-value"
NODE_ENV="development"

# OAuth (Google)
GOOGLE_ID="your_google_oauth_client_id"
GOOGLE_SECRET="your_google_oauth_client_secret"

# Stripe Test Keys
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY="pk_test_..."
STRIPE_SECRET_KEY="sk_test_..."