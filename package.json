        "update-env-secrets": "node scripts/generate-nextauth-secrets.js update-env",
        "check-env": "node check-environment.js",
        "deploy-test": "powershell -ExecutionPolicy Bypass -File deploy-test-branch.ps1",
        "deploy:amplify:production": "node scripts/deploy-amplify.js production",
        "deploy:amplify:development": "node scripts/deploy-amplify.js development",
        "deploy:amplify:staging": "node scripts/deploy-amplify.js staging",
        "amplify:list-environments": "node scripts/deploy-amplify.js list",
