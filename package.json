{
    "name": "event_nextjs",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev --turbopack",
        "build": "node scripts/validate-env.js && prisma generate && next build",
        "build:amplify": "next build",
        "build:amplify:production": "NODE_ENV=production next build",
        "build:amplify:development": "NODE_ENV=development next build",
        "build:test": "NODE_ENV=development next build",
        "start": "next start",
