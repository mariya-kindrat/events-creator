# Database
DATABASE_URL="your_database_connection_string"

# NextAuth
NEXTAUTH_SECRET="your_nextauth_secret"
NEXTAUTH_URL="https://your-app-domain.amplifyapp.com"

# OAuth Providers (if using)
GOOGLE_CLIENT_ID="your_google_client_id"
